{
	"check-coverage": true,
	"per-file": true,
	"lines": 80,
	"statements": 80,
	"functions": 80,
	"branches": 80,
	"include": [
		"**/*.js"
	],
	"exclude": [
		"**/*.spec.js",
		"src/utils/",
		"app.js"
	],
	"reporter": [
		"html"
	],
	"extension": [
		".jsx"
	],
	"cache": true,
	"all": true,
	"temp-directory": "./tmp",
	"report-dir": "../docs/backend/reports/coverage"
}