{
	"name": "tuto-r-",
	"version": "1.0.0",
	"description": "tutO(r)",
	"main": "index.js",
	"scripts": {
		"lint": "eslint ./src --fix",
		"start": "webpack-dev-server --mode development --open",
		"build": "webpack --mode production",
		"test": "jest",
		"jsdoc-all": "jsdoc -p -r -d ../docs/frontend/docs -c jsdoc-conf.json"
	},
	"jest": {
		"setupFiles": [
			"<rootDir>/src/setupTests.js"
		],
		"verbose": true,
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.{js,jsx}",
			"!**/node_modules/**",
			"!**/vendor/**/*.test.js",
			"!**/vendor/**"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"src/__tests__",
			"src/index.js",
			"webpack.config.js",
			"dist/"
		],
		"coverageDirectory": "../docs/frontend/reports/coverage",
		"coverageReporters": [
			"html"
		],
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 50,
				"statements": 80
			}
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.3",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^0.28.11",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^4.19.1",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-react": "^7.8.1",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^22.4.3",
		"jest-enzyme": "^6.0.0",
		"style-loader": "^0.21.0",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.1.3",
		"webpack-dev-server": "^3.1.4",
		"jsdoc": "^3.5.5"
	},
	"dependencies": {
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"lodash": "^4.17.5",
		"react-redux": "^4.0.0",
		"react-router-dom": "^4.0.0",
		"redux": "^3.0.4",
		"redux-promise": "^0.5.3"
	}
}
